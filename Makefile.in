prefix=@prefix@
exec_prefix=@exec_prefix@
srcdir = @srcdir@
libdir=@libdir@
CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@

CC=@CC@

all: ${srcdir}/base58.so

${srcdir}/base58.so: ${srcdir}/base58.c
	${CC} ${CPPFLAGS} ${CFLAGS} -Wall -shared -fPIC -o $@ $<

clean:
	rm -f ${srcdir}/base58.so

install:
	install --mode=644 --group=root --owner=root ${srcdir}/base58.so ${DESTDIR}${libdir}/tarantool
